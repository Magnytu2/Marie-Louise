{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "bs_blank",
  "version": "4.0.0",
  "description": "BS_blank is the blank Bootstrap 4.5 for Joomla 4 site template",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "template.bs_blank.ltr",
      "type": "style",
      "uri": "template.css",
      "dependencies": [
		"template.bs_blank.base",
		"template.bs_blank.bootstrap",
        "fontawesome",
        "template.bs_blank.googlefont"
      ]
    },
    {
      "name": "template.bs_blank.rtl",
      "type": "style",
      "uri": "template-rtl.css",
      "dependencies": [
		"template.bs_blank.base",
		"template.bs_blank.bootstrap",
        "fontawesome",
        "template.bs_blank.googlefont"
      ]
    },
	{
      "name": "template.bs_blank.base",
      "type": "style",
      "uri": "blog.css"
    },
	{
      "name": "template.bs_blank.bootstrap",
      "type": "style",
      "uri": "bootstrap.css"
    },
    {
      "name": "template.offline",
      "type": "style",
      "uri": "offline.css"
    },
    {
      "name": "template.active.language",
      "type": "style",
      "uri": "",
      "class": "LangActiveAssetItem",
      "dependencies": [
        "template.active"
      ]
    },
    {
      "name": "template.user",
      "type": "style",
      "uri": "user.css",
      "dependencies": [
        "template.active",
        "template.active.language"
      ]
    },
    {
      "name": "template.bs_blank.googlefont",
      "type": "style",
      "uri": "https://fonts.googleapis.com/css?family=Fira+Sans:400"
    },
    {
      "name": "template.bs_blank",
      "type": "script",
      "uri": "template.js",
      "dependencies": [
        "core",
        "bootstrap.js.bundle"
      ]
    },
    {
      "name": "template.active",
      "type": "script",
      "uri": "",
      "description": "A dummy asset to allow extensions to use it as a dependency to the active template",
      "dependencies": [
        "template.bs_blank"
      ]
    },
    {
      "name": "template.user",
      "type": "script",
      "uri":  "user.js",
      "dependencies": [
        "template.active"
      ]
    },

    {
      "name": "template.bs_blank.ltr",
      "type": "preset",
      "dependencies": [
        "template.bs_blank.ltr#style",
        "template.bs_blank#script"
      ]
    },
    {
      "name": "template.bs_blank.rtl",
      "type": "preset",
      "dependencies": [
        "template.bs_blank.rtl#style",
        "template.bs_blank#script"
      ]
    },
    {
      "name": "searchtools",
      "type": "style",
      "uri": "system/searchtools/searchtools.min.css"
    }
  ]
}
